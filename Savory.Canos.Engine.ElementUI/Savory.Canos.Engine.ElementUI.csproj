<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFrameworks>net452;netstandard2.0;netstandard2.1</TargetFrameworks>
    <AssemblyName>Savory.Canos.Engine.ElementUI</AssemblyName>
    <RootNamespace>Savory.Canos.Engine.ElementUI</RootNamespace>
    <Version>1.1.0</Version>
    <Authors>harriszhang@live.cn</Authors>
    <Company>harriszhang@live.cn</Company>
    <GeneratePackageOnBuild>true</GeneratePackageOnBuild>
    <DocumentationFile>bin\$(Configuration)\Savory.Canos.Engine.ElementUI.xml</DocumentationFile>
  </PropertyGroup>

  <ItemGroup>
    <PackageReference Include="Microsoft.CodeAnalysis.FxCopAnalyzers" Version="3.3.0">
      <PrivateAssets>all</PrivateAssets>
      <IncludeAssets>runtime; build; native; contentfiles; analyzers; buildtransitive</IncludeAssets>
    </PackageReference>
    <PackageReference Include="Savory.Canos.Engine" Version="1.1.10" />
    <PackageReference Include="Savory.Canos.Template.Vue" Version="3.2.0" />
    <PackageReference Include="Savory.CodeDom.Js" Version="1.8.4" />
    <PackageReference Include="Savory.CodeDom.Tag.Vue.ElementUI" Version="1.8.0" />
    <PackageReference Include="Savory.CodeDom.Vue.Engine" Version="1.8.0" />
  </ItemGroup>

</Project>
